{
  "id": "/examplevotingapp",
  "groups": [
    {
      "id": "/examplevotingapp/all",
      "apps": [
         {
            "id":    "voting-app",
            "cpus":  1,
            "mem":   128,
            "instances": 1,
            "acceptedResourceRoles": [
                "slave_public"
                ],
            "container": {
                    "type": "DOCKER",
                    "volumes": [
                    {
                        "containerPath": "/app",
                        "hostPath": "/example-voting-app/voting-app",
                        "mode": "RW"
                    }  
                    ],
                    "docker": {
                        "image": "docker/example-voting-app-voting-app",
                        "network": "BRIDGE",
                        "portMappings": [
                        {
                            "containerPort": 80,
                            "hostPort": 5000,
                            "servicePort": 10001,
                            "protocol": "tcp"
                        }
                        ],
                        "privileged": false,
                        "parameters": [],
                        "forcePullImage": false
                    }
            }
        },
        {
            "id":    "result-app",
            "cpus":  1,
            "mem":   128,
            "instances": 1,
            "acceptedResourceRoles": [
                "slave_public"
                ],
            "container": {
                    "type": "DOCKER",
                    "volumes": [
                    {
                        "containerPath": "/app",
                        "hostPath": "/example-voting-app/result-app",
                        "mode": "RW"
                    }
                    ],
                    "docker": {
                        "image": "docker/example-voting-app-result-app",
                        "network": "BRIDGE",
                        "portMappings": [
                        {
                            "containerPort": 80,
                            "hostPort": 5001,
                            "servicePort": 10002,
                            "protocol": "tcp"
                        }
                        ],
                        "privileged": false,
                        "parameters": [],
                        "forcePullImage": false
                    }
            }
        },
        {
            "id":    "worker",
            "cpus":  1,
            "mem":   256,
            "instances": 1,
            "container": {
                    "type": "DOCKER",
                    "volumes": [],
                    "docker": {
                        "image": "manomarks/worker",
                        "network": "BRIDGE",
                        "privileged": false,
                        "parameters": [],
                        "forcePullImage": false
                    }
            }
        },
        {
            "id":    "redis",
            "cpus":  1,
            "mem":   128,
            "instances": 1,
            "container": {
                    "type": "DOCKER",
                    "volumes": [],
                    "docker": {
                        "image": "redis:alpine",
                        "network": "BRIDGE",
                        "portMappings": [
                        {
                            "containerPort": 6379,
                            "hostPort": 6379,
                            "servicePort": 10003,
                            "protocol": "tcp"
                        }
                        ],
                        "privileged": false,
                        "parameters": [],
                        "forcePullImage": false
                    }
            }
        },
        {
            "id":    "db",
            "cpus":  1,
            "mem":   256,
            "instances": 1,
            "container": {
                        "type": "DOCKER",
                        "volumes": [
                            {
                            "containerPath": "/var/lib/postgresql/data",
                            "hostPath": "db-data",
                            "mode": "RW"
                            }
                        ],
                        "docker": {
                            "image": "postgres:9.4",
                            "network": "BRIDGE",
                            "privileged": false,
                            "parameters": [],
                            "forcePullImage": false
                        }
            }
        }
       ]
    }
  ]
}